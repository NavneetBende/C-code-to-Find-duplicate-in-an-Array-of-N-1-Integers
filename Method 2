#include<stdio.h>

void printRepeating(int arr[], int n)
{
    for (int i = 0; i < n; i++)
    {
        int index = (arr[i]-1) % n;
        arr[index] += n;
    }

    for (int i = 0; i < n; i++)
    {
        if(((arr[i]-1) / n) >= 2)
            printf("%d ",i+1);
    }
}

// Driver code
int main()
{
    int n;
    scanf("%d", &n);

    int arr[n];
    for(int i=0; i < n; i++)
        scanf("%d", &arr[i]);

    printf("The repeating elements are: \n");

    // Function call
    printRepeating(arr, n);
    return 0;
}
